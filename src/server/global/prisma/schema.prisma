generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Client {
  id       String  @id @unique
  name     String
  email    String  @unique
  active   Boolean
  photo    String
  role     roles @default(CLIENT)
  password String
}

enum roles {
  CLIENT
  ADMIN
  SUPERADMIN
}
